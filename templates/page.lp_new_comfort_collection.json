    "e809c555-6577-4219-9b81-be00aed9aa5c": {
      "type": "lp_text_on_image",
      "settings": {
        "title": "Psssst, neprozraďte nás!",
        "subtitle": "Improved",
        "text": "<p>O jarní kolekci Ahinsa shoes ještě skoro nikdo neví. Zatím jsme ji ukázali <strong>jen vám, našim zákazníkům<\/strong>.<br\/>Vyberte si jako první se slevou 25 %.<br\/>Boty už pro vás vyrábíme a odešleme je nejpozději v pondělí XX. března.<\/p>",
        "button_label": "",
        "button_link": "",
        "layout": "right",
        "divider": false,
        "quote": false
      }
    },
    "afadba8b-f130-41fc-8a37-2cc6ce89906e": {
